    "require": {
        "php": ">=5.3.0",
		"pguardiario/phpuri": "1.0",
        "electrolinux/phpquery": "dev-master",
		"john_schlick/simplehtmldom": "1.0"
    },
	"autoload": {
		"psr-0": {
			"PGuardiario\\": ["src/"]
		},
